{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["*"],
      "@actions": ["./src/actions"],
      "@assets": ["./assets"],
      "@data": ["./data"],
      "@json-schemas": ["./json-schemas"],
      "@services": ["./src/services"],
      "@shared-components": ["./src/components/shared"],
      "@src": ["./src"],
      "@hooks": ["./src/hooks"],
      "@utils": ["./src/utils"]
    },
    "allowJs": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["node_modules"]
}
