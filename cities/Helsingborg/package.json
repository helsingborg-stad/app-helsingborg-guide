    "react-native-linear-gradient": "^2.5.6",
    "react-native-maps": "^0.30.1",
    "react-native-matomo": "oskarhertzman/react-native-matomo",
    "react-native-permissions": "^3.2.0",
    "react-native-photo-view": "https://github.com/alwx/react-native-photo-view.git",
    "react-native-restart": "^0.0.23",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-safe-area-view": "2.0.0",
    "react-native-screens": "^3.10.1",
    "react-native-share": "7.3.2",
    "react-native-svg": "^12.1.1",
    "react-native-swiper": "^1.6.0-rc.3",
    "react-native-tab-view": "3.1.1",
    "react-native-vector-icons": "9.0.0",
    "react-native-version-number": "^0.3.6",
    "react-native-video": "5.0.2",
