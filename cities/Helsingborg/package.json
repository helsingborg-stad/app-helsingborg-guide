    "@react-navigation/stack": "^5.x",
    "ajv": "8.8.2",
    "axios": "0.24.0",
    "date-fns": "^2.9.0",
    "date-fns-tz": "^1.0.9",
    "geolib": "3.3.0",
    "guide-app": "file:../../guide-app",
    "haversine": "^1.1.1",
    "html-entities": "^2.3.2",
    "lodash": "4.17.21",
    "mercator-projection": "0.0.2",
    "metro-config": "^0.66.2",
