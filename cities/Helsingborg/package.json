{
  "name": "Helsingborg",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ../../node_modules/react-native/local-cli/cli.js start --reset-cache",
    "clean": "react-native-clean-project",
    "postinstall": "(cd ../../ && yarn postinstall)",
    "react:link": "react-native link",
    "translate": "node ./scripts/translate.js",
    "android:alpha:debug": "react-native run-android --variant alphaDebug --appIdSuffix alpha",
    "android:alpha:release": "react-native run-android --variant alphaRelease --appIdSuffix alpha",
    "android:dev:debug": "react-native run-android --variant devDebug",
    "android:store:debug": "react-native run-android --variant storeDebug",
    "android:store:release": "react-native run-android --variant storeRelease",
    "android:shake": "adb shell input keyevent 82",
    "android:clean": "cd android && ./gradlew clean && ./gradlew cleanBuildCache && cd .. ",
    "android:build:store:aab": "cd android && KEYSTORE_PROPERTIES=upload.properties && ./gradlew bundleStoreRelease && cd ..",
    "android:build:aab": "cd android && KEYSTORE_PROPERTIES=upload.properties && ./gradlew bundleRelease && cd ..",
    "android:build:apk": "cd android && ./gradlew assembleStoreRelease && cd ..",
    "android:release": "react-native run-android --variant storeRelease",
    "ios:device:debug": "react-native run-ios --device --configuration Debug",
    "ios:device:release": "react-native run-ios --device --configuration Release",
    "ios:simulator:debug": "react-native run-ios --simulator=\"iPhone 11 Pro\" --configuration Debug",
    "ios:simulator:release": "react-native run-ios --simulator --configuration Release",
    "android": "react-native run-android --variant devDebug",
    "ios": "react-native run-ios",
    "ios:device": "react-native run-ios --device",
    "ios:release": "react-native run-ios --configuration Release",
    "ios:pod:install": "cd ios && pod install && cd ..",
    "ios:clean": "cd ios && rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && pod cache clean --all && pod deintegrate && pod setup && pod install && cd ..",
    "ios:clean:light": "cd ios && xcodebuild clean && cd ..",
    "yarn:clean": "rm -rf node_modules && yarn cache clean --force && yarn install"
  },
  "dependencies": {
    "@notifee/react-native": "^4.0.1",
    "@react-native-async-storage/async-storage": "^1.15.15",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^7.1.7",
    "@react-native-community/progress-bar-android": "^1.0.4",
    "@react-native-community/progress-view": "^1.3.1",
